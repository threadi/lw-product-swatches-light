{
	"name": "threadi/product-swatches-light",
	"version": "2.1.0",
	"description": "This plugin adds swatches for WooCommerce products.",
	"authors": [
		{
			"name": "Thomas Zwirner",
			"email": "tz@laolaweb.com"
		}
	],
	"type": "wordpress-plugin",
	"homepage": "https://laolaweb.com",
	"minimum-stability": "stable",
	"scripts": {
		"post-install-cmd": [
			"composer dump-autoload"
		],
		"post-update-cmd": [
			"composer dump-autoload"
		],
		"test": "phpunit",
		"test-install": "bash bin/install-wp-tests.sh wordpress_test root 'debian' 127.0.0.1 latest",
		"test-install-during-build": "bash bin/install-wp-tests.sh wordpress_test root 'debian' 127.0.0.1 latest false true"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true,
			"phpstan/extension-installer": true
		},
		"optimize-autoloader": true,
		"classmap-authoritative": true,
		"apcu-autoloader": true,
		"prepend-autoloader": false
	},
	"autoload": {
		"psr-4": {
			"ProductSwatchesLight\\": "app/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"ProductSwatchesLight\\Tests\\": "tests/"
		}
	},
	"require": {
		"php": "^8.2",
		"threadi/easy-dialog-for-wordpress": "^1.0.1",
		"threadi/easy-setup-for-wordpress": "^1.2.3",
		"threadi/easy-transients-for-wordpress": "^1.2.1"
	},
	"require-dev": {
		"wp-coding-standards/wpcs": "^3.1.0",
		"pronamic/wp-documentor": "^1.3",
		"automattic/vipwpcs": "^3.0",
		"phpstan/phpstan": "^2.1",
		"szepeviktor/phpstan-wordpress": "^2.0",
		"phpstan/extension-installer": "^1.4",
		"php-stubs/wp-cli-stubs": "^2.11",
		"phpcompatibility/phpcompatibility-wp": "^2.1",
		"dealerdirect/phpcodesniffer-composer-installer": "^1.2",
		"yoast/phpunit-polyfills": "^1.0",
		"wp-phpunit/wp-phpunit": "^6.3"
	}
}
