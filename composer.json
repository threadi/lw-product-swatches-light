{
	"name": "threadi/product-swatches-light",
	"type": "wordpress-plugin",
	"homepage": "https://laolaweb.com",
	"minimum-stability": "stable",
	"scripts": {
		"post-install-cmd": [
			"\"vendor/bin/phpcs\" --config-set installed_paths vendor/wp-coding-standards/wpcs,vendor/phpcsstandards/phpcsutils,vendor/phpcsstandards/phpcsextra"
		],
		"post-update-cmd": [
			"\"vendor/bin/phpcs\" --config-set installed_paths vendor/wp-coding-standards/wpcs,vendor/phpcsstandards/phpcsutils,vendor/phpcsstandards/phpcsextra"
		]
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"require": {
		"php": "^8.0"
	},
	"require-dev": {
		"wp-coding-standards/wpcs": "^3.0.1"
	}
}
